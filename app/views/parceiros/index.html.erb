<h1>Listing parceiros</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Endereco</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @parceiros.each do |parceiro| %>
  <tr>
    <td><%= parceiro.nome %></td>
    <td><%= parceiro.endereco %></td>
    <td><%= link_to 'Show', parceiro %></td>
    <td><%= link_to 'Edit', edit_parceiro_path(parceiro) %></td>
    <td><%= link_to 'Destroy', parceiro, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Parceiro', new_parceiro_path %>
