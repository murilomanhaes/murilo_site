<div id="ladodireito">
  <h2>Concluir Suportes</h2><br>

<table>
  <tr>
    <th>Usuario</th>
    <th>Problema</th>
    <th>Data</th>
    <th></th>
  </tr>

  <% for suporte in @suportes %>

  <% if suporte.concluido? %>


  <% else %>
  <tr>
    <td><%= suporte.usuario.nome%></td>
    <td><%= suporte.problema %></td>
    <td><%= suporte.data.to_s_br %></td>
    <td><%= link_to 'Concluir', concluir_suporte_path(suporte) %></td>
   </tr>
  <%end%>


  <% end %>
</table>
</div>
